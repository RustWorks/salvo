{
  "name": "website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vuepress-cli build --clean-cache",
    "clean": "rimraf .vuepress/.temp .vuepress/.cache .vuepress/dist",
    "dev": "vuepress-cli dev --clean-cache",
    "serve": "anywhere -s -h localhost -d .vuepress/dist"
  },
  "dependencies": {
    "@vuepress/bundler-vite": "workspace:*",
    "@vuepress/bundler-webpack": "workspace:*",
    "@vuepress/cli": "workspace:*",
    "@vuepress/client": "workspace:*",
    "@vuepress/core": "workspace:*",
    "@vuepress/plugin-docsearch": "workspace:*",
    "@vuepress/plugin-google-analytics": "workspace:*",
    "@vuepress/plugin-register-components": "workspace:*",
    "@vuepress/plugin-shiki": "workspace:*",
    "@vuepress/theme-default": "workspace:*",
    "@vuepress/utils": "workspace:*",
    "anywhere": "^1.6.0",
    "sass-loader": "^13.1.0",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "vuepress": "^2.0.0-beta.53"
  }
}
